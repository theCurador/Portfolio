from sklearn import tree

#[height, weight, wingspan]
X = [[78,181,60], [81,199,83], [82,205,81], 
     [68,134,71], [74,182,73], [83,221,80],
     [70,168,68], [72,180,74], [77,159,78],
     [71,156,72], [82,210,81], [84,217,82]]
Y = ['SF', 'PF', 'C', 
     'PG', 'SG', 'C',
     'PG', 'PG', 'SF',
     'SG', 'PF', 'C']

clf = tree.DecisionTreeClassifier()

clf = clf.fit(X,Y)